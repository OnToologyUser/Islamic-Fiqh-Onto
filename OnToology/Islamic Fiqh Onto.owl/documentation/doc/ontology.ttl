@prefix : <http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/> .

<http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/تدفع_الى
:تدفع_الى rdf:type owl:ObjectProperty ;
          owl:inverseOf :يستفيد_من ;
          rdfs:domain :الزكاة ;
          rdfs:range :المستفيد .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/لأدائها
:لأدائها rdf:type owl:ObjectProperty ;
         owl:inverseOf :ناتجة_من ;
         rdfs:domain :الزكاة ;
         rdfs:range :الاثار_الايجابية .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/لتركها
:لتركها rdf:type owl:ObjectProperty ;
        owl:inverseOf :ناتجة_من ;
        rdfs:domain :الزكاة ;
        rdfs:range :الاثار_السلبية .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/منفذة_على
:منفذة_على rdf:type owl:ObjectProperty ;
           owl:inverseOf :يشترط_وجوب ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :شروط_الحج
                                       :شروط_الكفارة
                                       :شروط_النذر
                                       :شروط_الوقف
                                       :شروط_اليمين
                                     )
                       ] ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( :الحج
                                      :الكفارة
                                      :النذر
                                      :الوقف
                                      :اليمين
                                    )
                      ] .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/ناتجة_من
:ناتجة_من rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :الاثار_الايجابية
                                      :الاثار_السلبية
                                    )
                      ] ;
          rdfs:range :الزكاة .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/يؤدى_بواسطة
:يؤدى_بواسطة rdf:type owl:ObjectProperty ;
             owl:inverseOf :يؤدي ;
             rdfs:domain :الفقه_الاسلامي ;
             rdfs:range :المسلم .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/يؤدي
:يؤدي rdf:type owl:ObjectProperty ;
      rdfs:domain :المسلم ;
      rdfs:range :الفقه_الاسلامي .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/يستفيد_من
:يستفيد_من rdf:type owl:ObjectProperty ;
           rdfs:domain :المستفيد ;
           rdfs:range :الزكاة .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/يشترط_وجوب
:يشترط_وجوب rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :الحج
                                        :الكفارة
                                        :النذر
                                        :الوقف
                                        :اليمين
                                      )
                        ] ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :شروط_الحج
                                       :شروط_الكفارة
                                       :شروط_النذر
                                       :شروط_الوقف
                                       :شروط_اليمين
                                     )
                       ] .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/desirable
:desirable rdf:type owl:DatatypeProperty ;
           rdfs:domain :المسلم ;
           rdfs:range xsd:string ;
           rdfs:comment "اسم الخاصية في اللغة العربية (يستحب له)" .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/notDesirable
:notDesirable rdf:type owl:DatatypeProperty ;
              rdfs:domain :المسلم ;
              rdfs:range xsd:string ;
              rdfs:comment "اسم الخاصية في اللغة العربية (لا يستحب له)" .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/required
:required rdf:type owl:DatatypeProperty ;
          rdfs:domain :المسلم ;
          rdfs:range xsd:string ;
          rdfs:comment "اسم الخاصية في اللغة العربية (يشترط فيه)" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الاثار_الايجابية
:الاثار_الايجابية rdf:type owl:Class ;
                  rdfs:subClassOf :الزكاة .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الاثار_السلبية
:الاثار_السلبية rdf:type owl:Class ;
                rdfs:subClassOf :الزكاة .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الاحكام
:الاحكام rdf:type owl:Class ;
         rdfs:subClassOf :الفقه_الاسلامي ;
         rdfs:comment "الحكم الشرعي عبارة عن خطاب الشارع المتعلق بأفعال المكلفين. وتنقسم الأحكام إلى أقسام مختلفة، وأهمها: الأحكام الوضعية والتكليفة. كما تتفرع الأحكام الشرعية إلى أولية وثانوية." .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الايقاعات
:الايقاعات rdf:type owl:Class ;
           rdfs:subClassOf :الفقه_الاسلامي ;
           rdfs:comment "الإِيقاع: أوقعه فوقع. وأوقع فلانٌ بفلانٍ ما يكره: أي أحلَّه عليه وفي اصطلاح الفقهاء هو الإنشاء المستقل الذي لا يتوقف صحته ونفوذه على إنشاء مطاوع، فهو في مقابل العقد الذي حقيقته إنشاءان مرتبطان متلازمان" .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الحج
:الحج rdf:type owl:Class ;
      rdfs:subClassOf :العبادات ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :يشترط_وجوب ;
                        owl:someValuesFrom :شروط_الحج
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :منفذة_على ;
                        owl:allValuesFrom :شروط_الحج
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :يشترط_وجوب ;
                        owl:allValuesFrom :شروط_الحج
                      ] .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الزكاة
:الزكاة rdf:type owl:Class ;
        rdfs:subClassOf :العبادات ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :لأدائها ;
                          owl:someValuesFrom :الاثار_الايجابية
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :لتركها ;
                          owl:someValuesFrom :الاثار_السلبية
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :ناتجة_من ;
                          owl:allValuesFrom :الاثار_الايجابية
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :ناتجة_من ;
                          owl:allValuesFrom :الاثار_السلبية
                        ] .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/العبادات
:العبادات rdf:type owl:Class ;
          rdfs:subClassOf :الفقه_الاسلامي ;
          rdfs:comment "هو مصطلح دينيّ فقهيّ، له علاقة بتلك الأعمال الخاصّة الّتي أوجبها المولى تعالى على عبده،كما أنّ للعبادات معناً لغويّاً أعم من المعنى الفقهيّ الاصطلاحيّ، وهو مطلق الطّاعة مع الخضوع والتّذلّل لله" .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/العقود
:العقود rdf:type owl:Class ;
        rdfs:subClassOf :الفقه_الاسلامي ;
        rdfs:comment "هو ارتباط الإيجاب الصادر من أحد العاقدين بقبول الآخر على وجه يظهر أثره في المعقود عليه . في اللغة هو الربط والشد والإحكام والتوثيق والضمان والعهد، والجمع بين أطراف الشي‌ء." .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الفقه_الاسلامي
:الفقه_الاسلامي rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty :يؤدى_بواسطة ;
                                  owl:someValuesFrom :المسلم
                                ] ;
                owl:disjointUnionOf ( :الاحكام
                                      :الايقاعات
                                      :العبادات
                                      :العقود
                                    ) .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الكفارة
:الكفارة rdf:type owl:Class ;
         rdfs:subClassOf :الاحكام ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :يشترط_وجوب ;
                           owl:someValuesFrom :شروط_الكفارة
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :منفذة_على ;
                           owl:allValuesFrom :شروط_الكفارة
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :يشترط_وجوب ;
                           owl:allValuesFrom :شروط_الكفارة
                         ] .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/المستفيد
:المستفيد rdf:type owl:Class ;
          rdfs:subClassOf :المسلم ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :يستفيد_من ;
                            owl:someValuesFrom :الزكاة
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :تدفع_الى ;
                            owl:allValuesFrom :الزكاة
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :يستفيد_من ;
                            owl:allValuesFrom :الزكاة
                          ] ;
          rdfs:comment "هم الاشخاص الذين تحق لهم الزكاة." .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/المسلم
:المسلم rdf:type owl:Class .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/النذر
:النذر rdf:type owl:Class ;
       rdfs:subClassOf :الايقاعات ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :يشترط_وجوب ;
                         owl:someValuesFrom :شروط_النذر
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :منفذة_على ;
                         owl:allValuesFrom :شروط_النذر
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :يشترط_وجوب ;
                         owl:allValuesFrom :شروط_النذر
                       ] ;
       rdfs:comment "هو أن يجعل الشخص لله تعالى على ذمته فعل شيء أو تركه، بحيث يقوله بصيغة يجب التقيد بها وشروط خاصة تتعلّق بالنذر وبالشخص الناذر." .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الوقف
:الوقف rdf:type owl:Class ;
       rdfs:subClassOf :العقود ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :يشترط_وجوب ;
                         owl:someValuesFrom :شروط_الوقف
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :منفذة_على ;
                         owl:allValuesFrom :شروط_الوقف
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :يشترط_وجوب ;
                         owl:allValuesFrom :شروط_الوقف
                       ] .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/اليمين
:اليمين rdf:type owl:Class ;
        rdfs:subClassOf :الايقاعات ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :يشترط_وجوب ;
                          owl:someValuesFrom :شروط_اليمين
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :منفذة_على ;
                          owl:allValuesFrom :شروط_اليمين
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :يشترط_وجوب ;
                          owl:allValuesFrom :شروط_اليمين
                        ] ;
        rdfs:comment "هو الحلف بالله تعالى، أو بأحد أسمائه المختصه به" .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/شروط_الحج
:شروط_الحج rdf:type owl:Class ;
           rdfs:subClassOf :الحج .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/شروط_الكفارة
:شروط_الكفارة rdf:type owl:Class ;
              rdfs:subClassOf :الكفارة .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/شروط_النذر
:شروط_النذر rdf:type owl:Class ;
            rdfs:subClassOf :النذر .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/شروط_الوقف
:شروط_الوقف rdf:type owl:Class ;
            rdfs:subClassOf :الوقف .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/شروط_اليمين
:شروط_اليمين rdf:type owl:Class ;
             rdfs:subClassOf :اليمين .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/أداء_العمرة_والحج_في_اشهر_الحج
:أداء_العمرة_والحج_في_اشهر_الحج rdf:type owl:NamedIndividual ,
                                         :شروط_الحج ;
                                :منفذة_على :حج_التمتع .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/أطعام_ستّين_مسكين
:أطعام_ستّين_مسكين rdf:type owl:NamedIndividual ,
                            :شروط_الكفارة ;
                   :منفذة_على :كفارة_الافطار ,
                              :كفارة_الجمع ,
                              :كفارة_القتل ,
                              :كفارة_النذر ,
                              :كفارة_تعمد_الافطار ,
                              :كفارة_قتل_المؤمن ,
                              :كفارة_مخيرة ,
                              :كفارة_مرتبة .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/أن_يكون_لفظاً_دالاً_لمعنى_اليمين
:أن_يكون_لفظاً_دالاً_لمعنى_اليمين rdf:type owl:NamedIndividual ,
                                           :شروط_اليمين ;
                                  :منفذة_على :الأيمان_الصادقة ,
                                             :يمبن_المناشدة ,
                                             :يمين_العقد .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/ابن_السبيل
:ابن_السبيل rdf:type owl:NamedIndividual ,
                     :المستفيد ;
            owl:sameAs <https://www.wikidata.org/wiki/Q12486192> ;
            :يستفيد_من :زكاة_الاموال ,
                       :زكاة_الغلات_الاربع ,
                       :زكاة_الفطرة ,
                       :زكاة_الماشية ,
                       :زكاة_النقدين ,
                       :زكاة_اموال_التجارة ;
            :required "اعطاء الزكاة الى من لا تجب عليه نفقتهم" ,
                      "الأيمان" ,
                      "ان لا يصرف اموال الزكاة في الحرام" ,
                      "ان لا يكون هاشمي" ;
            rdfs:comment "وهو المسافر الذي نفدت نفقته أو تلفت راحلته ولا يتمكّن معه من الرجوع إلى بلده وإن كان غنيّاً فيه." .


###  https://www.wikidata.org/wiki/Q12486192
<https://www.wikidata.org/wiki/Q12486192> rdf:type owl:NamedIndividual .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/اداء_العمرة_والحج_في_سنة_واحدة
:اداء_العمرة_والحج_في_سنة_واحدة rdf:type owl:NamedIndividual ,
                                         :شروط_الحج ;
                                :منفذة_على :حج_التمتع .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/اداء_صلاة_الطواف
:اداء_صلاة_الطواف rdf:type owl:NamedIndividual ,
                           :شروط_الحج ;
                  :منفذة_على :حج_الأفراد ,
                             :حج_التمتع ,
                             :حج_القران ,
                             :عمرة_التمتع .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/اطعام_عشرة_مساكين
:اطعام_عشرة_مساكين rdf:type owl:NamedIndividual ,
                            :شروط_الكفارة ;
                   :منفذة_على :كفارة_البراءة ,
                              :كفارة_الترتيب_التخيير ,
                              :كفارة_النذر ,
                              :كفارة_اليمين ,
                              :كفارة_حنث_اليمين ,
                              :كفارة_لافطار_في_شهر_رمضان_بعد_الزوال ,
                              :كفارة_معينة .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/اكساء_عشرة_مساكين
:اكساء_عشرة_مساكين rdf:type owl:NamedIndividual ,
                            :شروط_الكفارة ;
                   :منفذة_على :كفارة_الترتيب_التخيير ,
                              :كفارة_النذر ,
                              :كفارة_اليمين ,
                              :كفارة_حنث_اليمين .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الأيمان_الصادقة
:الأيمان_الصادقة rdf:type owl:NamedIndividual ,
                          :اليمين ;
                 :يؤدى_بواسطة :الحالف ;
                 :يشترط_وجوب :أن_يكون_لفظاً_دالاً_لمعنى_اليمين ,
                             :الانجاز ,
                             :تأديته_بأسم_لله ,
                             :عدم_الحلف_بالبراءة_من_الأئمة_الطاهرين ,
                             :عدم_الحلف_بالبراءة_من_اللّه ,
                             :عدم_الحلف_بالبراءة_من_ذمة_الاسلام ,
                             :عدم_الحلف_بالبراءة_من_رسول_الله ;
                 rdfs:comment "يقصد بها ما يقع تأكيداً وتحقيقاً للإخبار عن تحقّق أمرٍ أو عدم تحقّقه في الماضي أو الحال أو الاستقبال, وهي ليست محرّمة ولكنّها مكروهة بحدّ ذاتها، فيكره للمكلّف أن يحلف على شيء صدقاً أو أن يحلف على صدق كلامه." .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الأيمان_الكاذبة
:الأيمان_الكاذبة rdf:type owl:NamedIndividual ,
                          :اليمين ;
                 rdfs:comment "يقصد بها ما يقع تأكيداً وتحقيقاً للإخبار عن تحقّق أمرٍ أو عدم تحقّقه في الماضي أو الحال أو الاستقبال, وهي محرّمة، بل قد تعتبر من المعاصي الكبيرة." .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الإحرام_من_أحد_المواقيت
:الإحرام_من_أحد_المواقيت rdf:type owl:NamedIndividual ,
                                  :شروط_الحج ;
                         :منفذة_على :حج_الأفراد ,
                                    :حج_القران ,
                                    :عمرة_التمتع .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الإحرام_من_مكّة
:الإحرام_من_مكّة rdf:type owl:NamedIndividual ,
                          :شروط_الحج ;
                 :منفذة_على :حج_التمتع .


###  http://www.semanticweb.org/asus/ontologies/2023/2/untitled-ontology-158/الاقباض
